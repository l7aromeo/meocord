{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    [
      "minify",
      {
        "keepFnName": true,
        "keepClassName": true
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "@src": "./src"
        }
      }
    ]
  ],
  "ignore": ["node_modules/discord.js"]
}
